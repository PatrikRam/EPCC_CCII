#include<iostream>
using namespace std;

template <class TIPOD>
void ar(TIPOD a[], TIPOD inicio,TIPOD final){
	double sum=0;
	for(int i=inicio; i<final; i++){
		cout<<a[i]<<" "<<endl;
		sum=sum+a[i];
		
	}
	cout<<"La suma en el rango dado es: "<<sum;	
}

int main(){
	int A[5]={2,1,3,6,5};
	ar(A, 1, 4);
		
}
